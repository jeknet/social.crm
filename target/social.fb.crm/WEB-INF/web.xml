<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Enapsys - SAP-CRM - Social bridge</display-name>
  <servlet>
  	<servlet-name>FacebookServlet</servlet-name>
  	<servlet-class>com.enapays.servlets.social.FacebookServlet</servlet-class> 
  </servlet>
  <servlet>
  	<servlet-name>TwitterServlet</servlet-name>
  	<servlet-class>com.enapays.servlets.social.TwitterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FacebookServlet</servlet-name>
  	<url-pattern>/Facebook/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TwitterServlet</servlet-name>
  	<url-pattern>/Twitter/*</url-pattern>
  </servlet-mapping>
</web-app>
